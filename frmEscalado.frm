Option Explicit

Dim MatrizDeProporciones() As ProporcionesControl

Private Sub Form_Load()
  ReDim MatrizDeProporciones(0 To Controls.Count - 1)
  InicializarMatriz

  Me.Height = Screen.Height / 2
  Me.Width = Screen.Width / 2
  Me.Top = (Screen.Height / 2) - (Me.Height) / 2
  Me.Left = (Screen.Width / 2) - (Me.Width) / 2
End Sub

Private Sub InicializarMatriz()
  Dim i As Integer
  For i = 0 To Controls.Count - 1
    If TypeOf Controls(i) Is Timer Then
      'no hace nada
    ElseIf TypeOf Controls(i) Is CommonDialog Then
      'no hace nada
    Else
      With MatrizDeProporciones(i)
        .ProporcionesAnchura = Controls(i).Width / ScaleWidth
        .ProporcionesAltura = Controls(i).Height / ScaleHeight
        .ProporcionesIzquierda = Controls(i).Left / ScaleWidth
        .ProporcionesSuperior = Controls(i).Top / ScaleHeight
      End With
    End If
  Next i
End Sub

Private Sub Form_Resize()
  Dim i As Integer
  For i = 0 To Controls.Count - 1
    If TypeOf Controls(i) Is Timer Then
      'no hace nada
    ElseIf TypeOf Controls(i) Is CommonDialog Then
      'no hace nada
    Else
      Controls(i).Move MatrizDeProporciones(i).ProporcionesIzquierda * ScaleWidth, _
                       MatrizDeProporciones(i).ProporcionesSuperior * ScaleHeight, _
                       MatrizDeProporciones(i).ProporcionesAnchura * ScaleWidth, _
                       MatrizDeProporciones(i).ProporcionesAltura * ScaleHeight
    End If
  Next i
End Sub
